@import "normalize"
@import "base"

$desktop = 1000px;

@media only screen and (min-width: $desktop) {
	@import "desktop"
}

@media only screen and (min-width: 769px) and (max-width: $desktop) {
	@import "small-desktop"
}

@media only screen and (min-width: 640px) and (max-width: $desktop) {
	@import "tablet"
}

@media only screen and (max-width: 640px) {
	@import "mobile"
}
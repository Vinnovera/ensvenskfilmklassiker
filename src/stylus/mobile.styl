.main-menu {
	display: none;
}


/*******************************************************/
/********************** INSTAGRAM **********************/
/*******************************************************/

.instagram-feed {	
	.top-content {
		position: relative;	
		.about-info {
			padding: 190px 10% 10% 10%;
			width: 80%;
		}
		
		
		.instagram-box {
			overflow-x: scroll;
			position: absolute;
			top: 90px;
			width: 100%;	
			
			img {
				height: 160px;
				width: 160px;
			}
			
			.all-images {
				width: (160*24)px;
		
				.more-images {
					height: 160px;
					width: 160px;
				}
			}		
		}
	}
}



